{
    "manifest_version": 3,
    "name": "GitHub Release Version Suggester",
    "version": "1.3",
    "description": "Automatically suggest the next release version on GitHub",
    "icons": {
        "128": "logo128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://github.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}